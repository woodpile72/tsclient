#: ../src/connect.c:316 ../src/connect.c:317
msgid "Logon Settings"
msgstr "Paramètres d'identification"

#: ../src/connect.c:324
msgid ""
"Type the name of the computer or choose a computer from the drop-down list."
msgstr "Saisissez le nom de l'ordinateur ou choisissez-le dans la liste déroulante."

#: ../src/connect.c:345
msgid "Compu_ter:"
msgstr "_Ordinateur :"

#: ../src/connect.c:352
msgid "Pro_tocol:"
msgstr "Pro_tocole :"

#: ../src/connect.c:359
msgid "_User Name:"
msgstr "_Identifiant :"

#: ../src/connect.c:366
msgid "Pass_word:"
msgstr "_Mot de passe :"

#: ../src/connect.c:373
msgid "Do_main:"
msgstr "_Domaine :"

#: ../src/connect.c:380
msgid "C_lient Hostname:"
msgstr "_Nom d'hôte du client :"

#: ../src/connect.c:387
msgid "Prot_ocol File:"
msgstr "Fichier de prot_ocole :"

#: ../src/connect.c:404
msgid "Enter the name or address of the remote system."
msgstr "Saisissez le nom ou l'adresse du système distant."

#: ../src/connect.c:410
msgid "Select the protocol to use for this connection."
msgstr "Sélectionnez le protocole à utiliser avec cette connexion."

#: ../src/connect.c:416
msgid ""
"RDP protocol is used to connect to Microsoft Windows 2000 and older systems."
msgstr ""
"Le protocole RDP est utilisé pour se connecter à Microsoft Windows 2000 "
"et aux systèmes plus ancients"

#: ../src/connect.c:423
msgid ""
"RDPv5 protocol is used to connect to Microsoft Windows XP and newer systems."
msgstr ""
"Le protocole RDPv5 est utilisé pour se connecter à Microsoft Windows XP "
"et aux systèmes plus récents"

#: ../src/connect.c:430
msgid ""
"VNC protocol is used to connect to systems sharing the desktop by using VNC."
msgstr ""
"Le protocole VNC est utilisé pour se connecter à des systèmes partageant le "
"bureau en utilisant VNC."

#: ../src/connect.c:441
msgid "XDMCP protocol uses Xnest to connect to servers running X."
msgstr "Le protocole XDMCP utilise Xnest pour se connecter à des serveurs "
"utilisant X."

#: ../src/connect.c:448
msgid "ICA protocol is used to connect to Citrix servers."
msgstr "Le protocole ICA est utilisé pour se connecter à des serveurs Citrix."

#: ../src/connect.c:463
msgid ""
"Enter the username for the remote system.\n"
"For VNC, enter the path to your saved vnc password file."
msgstr ""
"Saisissez l'identifiant pour le système distant.\n"
"Pour VNC, saisissez le chemin vers votre fichier de mot de passe vnc."

#: ../src/connect.c:471
msgid "Enter the password for the remote system."
msgstr "Saisissez le mot de passe pour le système distant."

#: ../src/connect.c:478
msgid "Enter the domain for the remote system."
msgstr "Saisissez le domaine pour le système distant."

#: ../src/connect.c:485
msgid "Enter the local hostname for this system."
msgstr "Saisissez le nom d'hôte local pour ce système."

#: ../src/connect.c:495
msgid ""
"Some protocols require a file containing settings. If required, enter the "
"path to the file here."
msgstr ""
"Certains protocoles requièrent un fichier contenant des paramètres. S'il est "
"requis saisissez le chemin vers ce fichier ici."

#: ../src/connect.c:511 ../src/connect.c:512
msgid "Remote Desktop Size"
msgstr "Taille du bureau distant"

#: ../src/connect.c:527
msgid "Use default screen size"
msgstr "Utiliser la taille d'écran par défaut"

#: ../src/connect.c:530
msgid "Use the default screen size."
msgstr "Utiliser la taille d'écran par défaut"

#: ../src/connect.c:535
msgid "Use specified screen size"
msgstr "Préciser une taille d'écran"

#: ../src/connect.c:538
msgid "Use the list to choose the screen size to use."
msgstr "Choisissez la taille d'écran à utiliser dans la liste."

#: ../src/connect.c:548
msgid "Operate in full screen mode"
msgstr "Fonctionner en plein écran"

#: ../src/connect.c:551
msgid "Work in full screen mode."
msgstr "Travailler en plein écran"

#: ../src/connect.c:559
msgid "Use alternate full screen switch (-F)"
msgstr "Utiliser l'argument alternatif (-F) pour le plein écran"

#: ../src/connect.c:562
msgid "Check this if you use rdesktop with the unified patches."
msgstr "Cochez si vous utilisez rdesktop en utilisant les correctifs unifiés "
"(« unified patches »)"

#: ../src/connect.c:570 ../src/connect.c:571
msgid "Colors"
msgstr "Couleurs"

#: ../src/connect.c:586
msgid "Use default color depth"
msgstr "Utiliser le nombre de couleurs par défaut"

#: ../src/connect.c:589
msgid "Use the default color depth."
msgstr "Utiliser le nombre de couleurs par défaut"

#: ../src/connect.c:594
msgid "Use specified color depth"
msgstr "Préciser un nombre de couleurs"

#: ../src/connect.c:597
msgid "Use the list to choose the color depth to use."
msgstr "Choisissez le nombre de couleurs à utiliser dans la liste."

#: ../src/connect.c:617
msgid "Remote Computer _Sound"
msgstr "_Son de l'ordinateur distant"

#: ../src/connect.c:618
msgid "Remote Computer Sound"
msgstr "Son de l'ordinateur distant"

#: ../src/connect.c:637 ../src/connect.c:640 ../src/connect.c:681
msgid "On the local computer"
msgstr "Sur l'ordinateur local"

#: ../src/connect.c:645 ../src/connect.c:648 ../src/connect.c:684
msgid "On the remote computer"
msgstr "Sur l'ordinateur distant"

#: ../src/connect.c:652 ../src/connect.c:655
msgid "Do not play"
msgstr "Pas de son"

#: ../src/connect.c:665
msgid "_Keyboard"
msgstr "_Clavier"

#: ../src/connect.c:666
msgid "Keyboard"
msgstr "Clavier"

#: ../src/connect.c:687
msgid "In full screen mode only"
msgstr "Seulement en plein écran"

#: ../src/connect.c:693
msgid ""
"Use the following keyboard language\n"
"(2 char keycode)"
msgstr ""
"Utiliser l'agencement de clavier suivant\n"
"(code à 2 caractères)"

#: ../src/connect.c:718
msgid ""
"Apply Windows key combinations\n"
"(for example ALT+TAB) (unsupported)"
msgstr ""
"Activer les combinaisons de touches Windows\n"
"(comme ALT+TAB) (non pris en charge)"

#: ../src/connect.c:738 ../src/connect.c:739
msgid "Start a Program"
msgstr "Lancer un programme"

#: ../src/connect.c:752
msgid "Start the following program on connection"
msgstr "Lancer ce programme à la connexion"

#: ../src/connect.c:758
msgid "Program path and filename"
msgstr "Nom et chemin du programme"

#: ../src/connect.c:769
msgid "Start in the following folder"
msgstr "Démarrer dans l'ordre suivant"

#: ../src/connect.c:788 ../src/connect.c:789
msgid "Optimize Performance"
msgstr "Optimiser les performances"

#: ../src/connect.c:807
msgid "Desktop background (u)"
msgstr "Fond d'écran (n)"

#: ../src/connect.c:810
msgid "Show content of window while dragging (u)"
msgstr "Afficher le contenu des fenêtres durant leur déplacement (n)."

#: ../src/connect.c:813
msgid "Menu and window animation (u)"
msgstr "Animation des menus et fenêtres (n)"

#: ../src/connect.c:816
msgid "Themes (u)"
msgstr "Thèmes (n)"

#: ../src/connect.c:819
msgid "Enable bitmap caching"
msgstr "Activer le cache des images"

#: ../src/connect.c:822
msgid "Do not send motion events"
msgstr "Ne pas envoyer les évènements clavier et souris"

#: ../src/connect.c:825
msgid "Enable window manager's key bindings"
msgstr "Activer les combinaisons de touches du gestionnaire de fenêtres"

#: ../src/connect.c:828
msgid "Hide window manager's decorations"
msgstr "Masquer les décorations du gestionnaire de fenêtres"

#: ../src/connect.c:831
msgid "Attach to console"
msgstr "Joindre à une console"

#: ../src/connect.c:835
msgid "Options available for optimizing performance"
msgstr "Options disponibles pour l'optimisation des performances"

